{
  "name": "fybf",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/apps/*",
    "packages/shared/*",
    "packages/config/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "build": "turbo run build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@fybf/shared.theme": "*"
  },
  "devDependencies": {
    "@fybf/config.eslint": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}